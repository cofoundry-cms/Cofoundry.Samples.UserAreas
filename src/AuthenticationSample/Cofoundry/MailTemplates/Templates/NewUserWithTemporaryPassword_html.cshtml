@model NewUserWithTemporaryPasswordMailTemplate
@{
    Layout = Model.GetLayoutFilePathForHtmlTemplate();
}
<h2>Welcome  @Model.User.DisplayName</h2>

<p>
    A ne member account has been created for you on the @Model.ApplicationName site.
</p>
<p>
    Your username is: @Model.User.Username<br />
    Your temporary password is: @Model.TemporaryPassword
</p>
<p>
    Sign in to your account here: <a href="@Model.SignInUrl">@Model.SignInUrl</a>
</p>